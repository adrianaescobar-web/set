{
  "name": "iot-arduino",
  "productName": "iot-arduino",
  "version": "1.0.0",
  "description": "Platadorma para recoleccion de datos desde arduino",
  "main": "src/",
  "scripts": {
    "start": "electron-forge start --inspect-electron",
    "test": "jest --watchAll",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "install": "electron-rebuild --version 1.6.0 --force --module-dir ./"
  },
  "keywords": [],
  "author": "Andrés Marrugo",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "iot_arduino"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": "Andrés Marrugo"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "iotarduino"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "ease-component": "^1.0.0",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-root-path": "^1.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "johnny-five": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "^5.0.0",
    "socket.io-client": "^2.2.0",
    "usb": "^1.5.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt": "^1.4.13",
    "electron-prebuilt-compile": "4.0.0",
    "electron-rebuild": "^1.8.4",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "jest": "^24.8.0"
  },
  "proxy": "http://localhost:3000"
}
